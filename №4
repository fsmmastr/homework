 #1.4 Intersection of sets (intersect)
def intersect(A, B):
    intersection = A & B  # Пересечение множеств
    result = []

    # Поиск элементов в пересечении и добавление их в результат
    for i in range(32):
        if intersection & (1 << i):  # Проверка i-го бита на принадлежность пересечению
            result.append(i)

    return result

A = int(input("Введите множество A в виде 32-битного числа: "))
B = int(input("Введите множество B в виде 32-битного числа: "))

intersection = intersect(A, B)
print("Элементы множества A пересекаются с элементами множества B:", intersection)

